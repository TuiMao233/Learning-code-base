{
	"name": "create-mpvue-view",
  "displayName": "create-mpvue-view",
  "keywords": ["mpvue", "page", "components"],
  "publisher": "mrmaoddxxaa",
  "description": "create mpvue-page and mpvue-components",
  "version": "1.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:create-mpvue-view.createPage",
        "onCommand:create-mpvue-view.createComponent"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "create-mpvue-view.createPage",
				"title": "创建mpvue页面"
			},
			{
				"command": "create-mpvue-view.createComponent",
				"title": "创建mpvue组件"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"group": "navigation@9",
					"command": "create-mpvue-view.createPage",
					"title": "创建mpvue页面"
				},
				{
					"group": "navigation@9",
					"command": "create-mpvue-view.createComponent",
					"title": "创建mpvue组件"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
